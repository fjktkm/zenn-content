{
	"name": "Node.js",
	"image": "mcr.microsoft.com/devcontainers/javascript-node:1-20-bullseye",
	"mounts": [
		{
			"type": "volume",
			"target": "${containerWorkspaceFolder}/node_modules"
		}
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"zenn.zenn-preview",
				"mhutchie.git-graph"
			]
		}
	},
	"postCreateCommand": "yarn install",
	"remoteUser": "root"
}